<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <!-- Insert sample enrollments with different statuses -->
    <changeSet id="2026-01-11-insert-sample-enrollments-01" author="toanld">
        <!-- Enrollments with Registered status -->
        <insert tableName="enrollment">
            <column name="student_id" value="1"/>
            <column name="course_id" value="1"/>
            <column name="status" value="Registered"/>
        </insert>

        <insert tableName="enrollment">
            <column name="student_id" value="2"/>
            <column name="course_id" value="2"/>
            <column name="status" value="Registered"/>
        </insert>

        <insert tableName="enrollment">
            <column name="student_id" value="3"/>
            <column name="course_id" value="3"/>
            <column name="status" value="Registered"/>
        </insert>

        <!-- Enrollments with Success status -->
        <insert tableName="enrollment">
            <column name="student_id" value="1"/>
            <column name="course_id" value="2"/>
            <column name="status" value="Success"/>
        </insert>

        <insert tableName="enrollment">
            <column name="student_id" value="2"/>
            <column name="course_id" value="3"/>
            <column name="status" value="Success"/>
        </insert>

        <insert tableName="enrollment">
            <column name="student_id" value="4"/>
            <column name="course_id" value="1"/>
            <column name="status" value="Success"/>
        </insert>

        <!-- Enrollments with Reject status -->
        <insert tableName="enrollment">
            <column name="student_id" value="3"/>
            <column name="course_id" value="1"/>
            <column name="status" value="Reject"/>
        </insert>

        <insert tableName="enrollment">
            <column name="student_id" value="5"/>
            <column name="course_id" value="2"/>
            <column name="status" value="Reject"/>
        </insert>

        <insert tableName="enrollment">
            <column name="student_id" value="4"/>
            <column name="course_id" value="3"/>
            <column name="status" value="Reject"/>
        </insert>
    </changeSet>

</databaseChangeLog>
